func f(arg1, arg2) : {
    return loop1(arg1);
    return loop2(arg2);
    return [] :: 3;
}

func loop1(arg1) : {
    return g();
}

func g() : {
    return f([][~true], []);
}

func loop2(arg2) : {
    return k();
}

func k() : {
    return f([], []);
}

func ret() : {
    return [[]];
}

main : f([[1, 2]][2 > 4], f([], [])[ret()]);

