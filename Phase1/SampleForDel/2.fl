def list_printer(l, i)
    if (i != len(l))
        puts(l[i]);
        list_printer(l, i + 1);
    end
end

def printList(l, i)
    puts(l[i][0]);
    puts(l[i][1]);
    printList(l, i - 1);
end

def create2dList(l, i, j)
    if(i != 10)
        create2dList(l[i] << i << j * 2, i + 1, j + 1);
    else
        printList(l, i);
    end
end

