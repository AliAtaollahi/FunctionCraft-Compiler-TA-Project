    return a + b;
end

pattern pat1(x)
    | (x <= 1) = 0
    | (x == 2) = pat1(x-1)
    | (x > 2) = pat1(x-1) + pat1(x-2) + 1
;

pattern pat2(n)
    | (n == 1) = 1
    | (n > 1) = n * pat2(n-1)
;

def factorPattern()
    facResult = pat2.match(10);
end

